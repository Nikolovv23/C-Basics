// Create a program that reads a password (text) entered by the user and checks that the password entered matches the phrase "s3cr3t! P@ssw0rd". 
//In case of coincidence, display "Welcome". In case of inconsistency, display "Wrong password!". 

using System;

namespace PasswordGuess
{
    internal class Program
    {
        static void Main(string[] args)
        {
            string password= Console.ReadLine(); 
           
            if (password == "s3cr3t!P@ssw0rd")
            {
                Console.WriteLine("Welcome");
            }
            else
            {
                Console.WriteLine("Wrong password!");
            }
        }
    }
}
