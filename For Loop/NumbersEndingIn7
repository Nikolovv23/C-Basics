// Write a program that prints numbers in the range 1 to 1000 that end in 7.


using System;
namespace NumbersEndingIn7
{
    internal class Program
    {
        static void Main(string[] args)
        {
           
          for (int i = 1;  i <= 1000; i ++)
            {
                if (i % 10 == 7)
                {
                    Console.WriteLine(i);
                }
                
            }
        }

    }
}
