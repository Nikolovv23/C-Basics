// Write a program that reads a number n entered by the user and prints the even powers of 2 ≤ 2n: 20, 22, 24, 26, …, 2n.  

using System;
namespace EvenPowersOf2
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int a = int.Parse(Console.ReadLine());
            int num = 1; 
            for (int i = 0 ; i <= a ; i += 2 )
            {
                Console.WriteLine(num);
                num = num * 2 * 2;
            } 
        }
    }
}
