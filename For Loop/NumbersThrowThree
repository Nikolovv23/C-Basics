// Create a program that reads a number n entered by the user and prints the numbers from 1 to n through 3.

using System;
namespace NumbersThrowThree
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int a = int.Parse(Console.ReadLine());
            for (int i = 1 ; i <= a ; i += 3)
            {
                Console.WriteLine(i);
            } 
        }
    }
}
